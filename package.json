{
	"name": "pandora-server-directory",
	"version": "0.1.0",
	"private": true,
	"repository": "github:Project-Pandora-Game/pandora-server-directory",
	"license": "GPL-3.0-or-later",
	"engines": {
		"node": "16.x"
	},
	"scripts": {
		"build": "tsc -p .",
		"dev": "nodemon -e ts --ignore dist --exec \"yarn build && yarn start\"",
		"start": "node --enable-source-maps -r dotenv/config dist/index.js",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix .",
		"postinstall": "node scripts/postinstall.js"
	},
	"devDependencies": {
		"@types/node": "^16.6.1",
		"@typescript-eslint/eslint-plugin": "^4.29.0",
		"@typescript-eslint/parser": "^4.29.0",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.15",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"dotenv": "^10.0.0",
		"pandora-common": "github:Project-Pandora-Game/pandora-common"
	}
}
