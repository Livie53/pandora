	"scripts": {
		"prebuild": "rimraf dist",
		"build": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --env prod",
		"predev": "rimraf dist",
		"dev": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack serve",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix .",
		"type-check": "tsc -p . --noEmit",
		"type-check:test": "tsc -p ./tsconfig.test.json --noEmit",
		"type-check:src": "tsc -p ./tsconfig.src.json --noEmit",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"start": "yarn dev",
		"postinstall": "node scripts/postinstall.js"
